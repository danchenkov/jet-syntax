{
    "name": "jet",
    "displayName": "jet",
    "description": "Jet Templating Language",
    "publisher": "AlexeiDanchenkov",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:jet.helloWorld"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "jet.helloWorld",
                "title": "Hello World"
            }
        ],
        "languages": [
            {
                "id": "jet",
                "aliases": [
                    "HTML Template (Jet)",
                    "jet"
                ],
                "extensions": [
                    ".jet"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jet",
                "scopeName": "text.html.jet",
                "path": "./syntaxes/jet.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.html": "html",
                    "meta.embedded.block.yaml": "yaml",
                    "meta.embedded.block.javascript": "javascript"
                }
            }
        ]
    },
    "dependencies": {
        "vscode": "^0.9.9"
    },
    "scripts": {},
    "devDependencies": {
        "@types/vscode": "^1.32.0"
    }
}
